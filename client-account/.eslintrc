{
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential"
    ],
    "rules": {
        "no-unused-vars": [
            1,
            {
                "args": "all",
                "argsIgnorePattern": "^_"
            }
        ]
    },
    "globals": {
        "defineProps": "readonly",
        "defineEmits": "readonly",
        "require": "readonly"
    }
}